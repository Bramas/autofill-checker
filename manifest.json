{
  "manifest_version": 2,

  "name": "Autofill Checker",
  "description": "Check what the chrome autofill actually filled",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "persistent": false,
    "scripts": ["bg.js"]
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "background"
],
"icons": { "192": "icon.png" }
}
