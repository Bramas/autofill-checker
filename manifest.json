{
  "manifest_version": 2,

  "name": "Autofill Checker",
  "description": "Check what the chrome autofill actually filled",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["content.js"],
        "run_at": "document_start",
        "all_frames": false
    }
  ],
  "background": {
    "persistent": false,
    "scripts": ["bg.js"]
  },
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "background"
],
"icons": { "192": "icon.png" }
}
